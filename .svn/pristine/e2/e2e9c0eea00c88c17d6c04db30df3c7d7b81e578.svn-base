package files;

import directory.Directory;

public class File {

    protected String name;
    protected Directory parent;
    
    public File(String name, Directory parent) {
        this.name = name;    
        this.parent = parent;    
    }
    
    public String getName() {
        return this.name;
    }

    public Directory getParent() {
        return this.parent;
    }
}
